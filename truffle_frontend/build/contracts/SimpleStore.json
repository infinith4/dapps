  "source": "pragma solidity ^0.4.22;\n\ncontract SimpleStore {\n  uint value;\n  event NewValueSet(uint _value);\n  function set(uint _value) public {\n    value = _value;\n    emit NewValueSet(value);\n  }\n  function get() public view returns (uint) {\n    return value;\n  }\n}",
