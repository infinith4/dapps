  "source": "pragma solidity >=0.5.0 <0.6.0;\n \ncontract HelloWorld {\n  string word;\n \n  constructor(string memory _word) public {\n      word = _word;\n  }\n \n  function getWord() public view returns(string memory) {\n      return word;\n  }\n  \n  function changeWord(string memory _word) public {\n      word = _word;\n  }\n \n}",
